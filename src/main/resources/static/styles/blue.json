[
  {
    "id": "background",
    "type": "background",
    "layout": {
      "visibility": "visible"
    },
    "paint": {
      "background-color": "#00263D"
    }
  },
  {
    "id": "landuse",
    "type": "fill",
    "source": "osm-tiles",
    "source-layer": "landuse",
    "paint": {
      "fill-color": "#444444"
    }
  },
  {
    "id": "landcover",
    "type": "fill",
    "source": "osm-tiles",
    "source-layer": "landcover",
    "paint": {
      "fill-color": "#43ad7f",
      "fill-opacity": 0.3
    }
  },
  {
    "id": "water",
    "type": "fill",
    "source": "osm-tiles",
    "source-layer": "water",
    "paint": {
      "fill-color": "#015179"
    }
  },
  {
    "id": "building",
    "type": "fill",
    "source": "osm-tiles",
    "source-layer": "building",
    "paint": {
      "fill-color": "#05A2F0"
    }
  },
  {
    "id": "building-extrusion",
    "type": "fill-extrusion",
    "source": "osm-tiles",
    "source-layer": "building",
    "minzoom": 15,
    "paint": {
      "fill-extrusion-color": [
        "interpolate",
        [
          "linear"
        ],
        [
          "zoom"
        ],
        8,
        "#05A2F0",
        22,
        "#077fbb"
      ],
      "fill-extrusion-height": [
        "interpolate",
        [
          "linear"
        ],
        [
          "zoom"
        ],
        15,
        0,
        16,
        [
          "get",
          "render_height"
        ]
      ],
      "fill-extrusion-base": [
        "case",
        [
          ">=",
          [
            "get",
            "zoom"
          ],
          16
        ],
        [
          "get",
          "render_min_height"
        ],
        0
      ]
    }
  },
  {
    "id": "transportation_name",
    "type": "line",
    "source": "osm-tiles",
    "source-layer": "transportation_name"
  },
  {
    "id": "transportation",
    "type": "line",
    "source": "osm-tiles",
    "source-layer": "transportation",
    "paint": {
      "line-color": "#06789D",
      "line-opacity": 1,
      "line-width": 1
    }
  },
  {
    "id": "place",
    "type": "symbol",
    "source": "osm-tiles",
    "source-layer": "place",
    "layout": {
      "text-field": [
        "get",
        "name"
      ],
      "text-size": 12,
      "text-allow-overlap": false,
      "text-offset": [
        0,
        0
      ],
      "text-anchor": "center",
      "text-justify": "center",
      "text-font": [
        "Arial Unicode MS Regular"
      ]
    },
    "paint": {
      "text-halo-color": "#444444",
      "text-halo-width": 1,
      "text-color": "#FFFFFF"
    }
  },
  {
    "id": "poi",
    "type": "symbol",
    "source": "osm-tiles",
    "source-layer": "poi",
    "layout": {
