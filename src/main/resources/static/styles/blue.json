    "id": "background",
    "type": "background",
    "layout": {
      "visibility": "visible"
    },
    "paint": {
      "background-color": "#00263D"
    }
  },
  {
    "id": "landuse",
    "type": "fill",
    "source": "osm-tiles",
    "source-layer": "landuse",
    "paint": {
      "fill-color": "#444444"
    }
  },
