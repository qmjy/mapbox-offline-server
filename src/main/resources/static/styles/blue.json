    }
  },
  {
    "id": "building-extrusion",
    "type": "fill-extrusion",
    "source": "osm-tiles",
    "source-layer": "building",
    "minzoom": 15,
    "paint": {
      "fill-extrusion-color": [
        "interpolate",
        ["linear"],
        ["zoom"],8, "#05A2F0", 22, "#077fbb"
      ],
      "fill-extrusion-height": [
        "interpolate", ["linear"], ["zoom"],
        15, 0,
        16, ["get","render_height"]
      ],
      "fill-extrusion-base": ["get", "render_min_height"]
    }
  },
  {
